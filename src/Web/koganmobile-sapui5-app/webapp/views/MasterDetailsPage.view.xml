<mvc:View
    displayBlock="true"
    controllerName="kogan.mobile.controller.MasterDetailsPage"
    height="100%"
    xmlns="sap.uxap"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns:f="sap.f"
    xmlns:semantic="sap.m.semantic"
    xmlns:form="sap.ui.layout.form"
    core:require="{
		StringUtils: 'kogan/mobile/utils/StringUtils'
	}"
>
    <f:FlexibleColumnLayout
        id="flexColumnLayout"
        stateChange="onStateChanged"
        layout="{PAGE>/Layout}"
        backgroundDesign="Translucent"
    >
        <f:beginColumnPages />
        <f:midColumnPages>
            <ObjectPageLayout>
                <headerTitle>
                    <ObjectPageHeader>
                        <m:Title text="lol" />
                    </ObjectPageHeader>
                </headerTitle>
            </ObjectPageLayout>
        </f:midColumnPages>
    </f:FlexibleColumnLayout>
</mvc:View>